{
  "name": "meditorial/mailkit-api",
  "type": "library",
  "description": "Mailkit api client library",
  "keywords": ["igloonet", "mailkit", "mail"],
  "license": ["BSD-3-Clause"],
  "authors": [
    {
      "name": "Miroslav Čížek",
      "email": "cizek@iglonet.cz"
    },
    {
      "name": "Roman Braciník",
      "email": "fejjo@igloonet.cz"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "nette/utils": "^3.2",
    "ext-xmlrpc": "*"
  },
  "require-dev": {
    "nette/tester": "^2.1",
    "tracy/tracy": "^2.5",
    "phpstan/phpstan": "1.*",
    "phpstan/phpstan-phpunit": "1.*",
    "phan/phan": "^5.3",
    "rector/rector": "^0.13.9"
  },
  "autoload": {
    "psr-4": {
      "Igloonet\\MailkitApi\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["tests/IgloonetTests"]
  },
  "scripts": {
    "stan": [
      "phpstan analyze"
    ]
  }
}
